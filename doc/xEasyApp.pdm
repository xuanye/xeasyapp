-- =============================================
-- Author:		&lt;xuanye&gt;
-- Create date: &lt;2011-06-01&gt;
-- Description:	&lt;删除用户信息&gt;
-- =============================================
Create PROCEDURE SP_DeleteUserInfo
	@UserUID varchar(50)
AS
BEGIN

	SET NOCOUNT ON;
	DELETE FROM UserDeptRelation WHERE UserUID=@UserUID
	DELETE FROM RoleUserRelation WHERE UserUID=@UserUID
	DELETE FROM UserInfos WHERE UserUID=@UserUID
END
GO
</a:Procedure.Text>
</o:Procedure>
